{
  "manifest_version": 2,

  "name": "Cefalo Time Tracker",
  "description": "Do the calculations of office time for you.",
  "version": "1.1.1",

  "icons": {
    "128": "icon.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://attendance.cefalolab.com/",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": ["js/background.js"]
  },
  "web_accessible_resources": [
    "images/done.png"
  ]
}